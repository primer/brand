{
  "name": "@primer/react-brand",
  "version": "0.0.3",
  "author": "GitHub, Inc.",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/primer/react-brand/issues"
  },
  "homepage": "https://github.com/primer/react-brand#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/primer/react-brand.git"
  },
  "engines": {
    "npm": ">=8.0.0",
    "node": ">=14.0.0"
  },
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "npm run clean && npm run build:css-types && npm run build:css-vars && webpack",
    "build:css-vars": "ts-node --skipIgnore ./scripts/build-css-variables.ts",
    "build:types": "tsc --project tsconfig.build.json",
    "build:storybook": "scripts/build-storybook",
    "build:docs": "scripts/build-docs",
    "build:css-types": "tcm -p 'src/**/*.css' .",
    "check": "tsc --noEmit",
    "clean": "rm -rf lib",
    "format": "prettier --check '**/*.{js,jsx,ts,tsx,md,mdx,css,json}'",
    "format:fix": "prettier --write '**/*.{js,jsx,ts,tsx,md,mdx,css,json}'",
    "lint": "eslint '**/*.{js,ts,tsx,md,mdx}' --max-warnings=0",
    "lint:fix": "npm run lint -- --fix",
    "postinstall": "cd docs && npm i",
    "release": "npm run build && changeset publish",
    "start": "npm run build && npm run start:storybook & npm run start:docs",
    "start:storybook": "start-storybook -p 6006",
    "start:docs": "cd docs && npm run develop",
    "start:css-types": "tcm -w -p 'src/**/*.css' .",
    "test": "jest"
  },
  "prettier": "@github/prettier-config",
  "peerDependencies": {
    "react": ">=17.0.0 <= 18.0.0",
    "react-dom": ">=17.0.0 <= 18.0.0"
  },
  "devDependencies": {
    "@changesets/changelog-github": "^0.4.3",
    "@changesets/cli": "^2.21.0",
    "@github/prettier-config": "^0.0.4",
    "@primer/octicons-react": "^17.0.0",
    "@primer/primitives": "^7.5.1",
    "@storybook/addon-actions": "^6.4.19",
    "@storybook/addon-essentials": "^6.4.19",
    "@storybook/addon-interactions": "^6.4.19",
    "@storybook/addon-links": "^6.4.19",
    "@storybook/builder-webpack5": "^6.4.19",
    "@storybook/manager-webpack5": "^6.4.19",
    "@storybook/react": "^6.4.19",
    "@storybook/testing-library": "^0.0.9",
    "@testing-library/dom": "^8.11.3",
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.4",
    "@testing-library/react-hooks": "^7.0.2",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.4.1",
    "@types/react": "^17.0.37",
    "@types/react-dom": "^17.0.11",
    "@types/webpack": "^4.41.25",
    "@typescript-eslint/eslint-plugin": "^5.17.0",
    "@typescript-eslint/parser": "^5.17.0",
    "autoprefixer": "^9.8.8",
    "css-loader": "^5.2.7",
    "cross-env": "7.0.2",
    "eslint": "^8.12.0",
    "eslint-plugin-github": "^4.3.6",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-mdx": "^1.17.0",
    "eslint-plugin-primer-react": "^0.7.4",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.4.0",
    "eslint-import-resolver-typescript": "^2.7.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.5.1",
    "jest-axe": "^6.0.0",
    "mini-css-extract-plugin": "^2.6.0",
    "postcss": "^8.4.7",
    "postcss-loader": "^6.2.1",
    "postcss-preset-env": "^7.4.2",
    "prettier": "^2.5.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "storybook-css-modules-preset": "^1.1.1",
    "style-loader": "^3.3.1",
    "ts-jest": "^27.1.3",
    "ts-loader": "^9.2.7",
    "ts-node": "^10.7.0",
    "typed-css-modules": "^0.7.0",
    "typescript": "^4.6.3",
    "typescript-plugin-css-modules": "^3.4.0",
    "webpack": "^5.70.0",
    "webpack-cli": "^4.9.2"
  },
  "dependencies": {
    "clsx": "^1.1.1"
  }
}
