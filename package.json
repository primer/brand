{
  "name": "@primer/react-brand",
  "description": "Primer Brand is a GitHub's design system for creating React-based marketing websites and digital experiences.",
  "version": "0.4.0",
  "author": "GitHub, Inc.",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/primer/brand/issues"
  },
  "homepage": "https://primer.style/brand",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/primer/brand.git"
  },
  "keywords": [
    "primer",
    "react",
    "components",
    "library",
    "design-system"
  ],
  "engines": {
    "npm": ">=8.0.0",
    "node": ">=16.0.0"
  },
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib",
    "fonts"
  ],
  "scripts": {
    "build": "npm run clean && npm run build:css-types && npm run build:tokens && webpack",
    "build:css-types": "tcm -p 'src/**/*.module.css' .",
    "build:types": "tsc --project tsconfig.build.json",
    "build:storybook": "scripts/build-storybook",
    "build:storybook:preview": "scripts/build-storybook preview",
    "build:docs": "scripts/build-docs",
    "build:docs:preview": "scripts/build-docs preview",
    "build:tokens": "node scripts/build-tokens.js",
    "check": "tsc --noEmit",
    "clean": "rm -rf lib",
    "format": "prettier --check '**/*.{js,jsx,ts,tsx,md,mdx,css,json}'",
    "format:fix": "prettier --write '**/*.{js,jsx,ts,tsx,md,mdx,css,json}'",
    "lint": "eslint '**/*.{js,ts,tsx,md,mdx}' --max-warnings=0",
    "lint:fix": "npm run lint -- --fix",
    "release": "npm run build && changeset publish",
    "start": "npm run build && npm run start:storybook & npm run start:docs",
    "start:storybook": "start-storybook -p 6006",
    "start:docs": "cd docs && npm run develop",
    "start:css-types": "tcm -w -p 'src/**/*.css' .",
    "test": "jest",
    "test:visual": "npx build-storybook && scripts/playwright/run-visual-tests",
    "test:visual:generate": "npm run build:docs && ts-node scripts/playwright/playwright.generate-tests.ts",
    "test:visual:update-snapshots": "npx build-storybook && scripts/playwright/update-visual-snapshots"
  },
  "peerDependencies": {
    "react": ">=17 <= 18",
    "react-dom": ">=17 <= 18"
  },
  "devDependencies": {
    "@changesets/changelog-github": "^0.4.3",
    "@changesets/cli": "^2.21.0",
    "@github/prettier-config": "^0.0.4",
    "@playwright/test": "^1.24.0",
    "@primer/octicons-react": "^17.3.0",
    "@primer/primitives": "^7.9.0",
    "@reach/auto-id": "^0.17.0",
    "@storybook/addon-a11y": "^6.5.9",
    "@storybook/addon-actions": "^6.5.6",
    "@storybook/addon-essentials": "^6.5.6",
    "@storybook/addon-interactions": "^6.5.9",
    "@storybook/addon-links": "^6.5.6",
    "@storybook/builder-webpack5": "^6.5.6",
    "@storybook/jest": "^0.0.10",
    "@storybook/manager-webpack5": "^6.5.6",
    "@storybook/react": "^6.5.6",
    "@storybook/test-runner": "^0.5.0",
    "@storybook/testing-library": "^0.0.11",
    "@testing-library/dom": "^8.11.3",
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.4",
    "@testing-library/react-hooks": "^7.0.2",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.4.1",
    "@types/react": "^17.0.50",
    "@types/react-dom": "^17.0.11",
    "@types/webpack": "^4.41.25",
    "@typescript-eslint/eslint-plugin": "^5.17.0",
    "@typescript-eslint/parser": "^5.17.0",
    "autoprefixer": "^9.8.8",
    "axe-playwright": "^1.1.11",
    "color2k": "^2.0.0",
    "cross-env": "7.0.2",
    "css-loader": "^6.7.1",
    "eslint": "^8.12.0",
    "eslint-import-resolver-typescript": "^2.7.0",
    "eslint-plugin-github": "^4.3.6",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-mdx": "^1.17.0",
    "eslint-plugin-primer-react": "^0.7.4",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.4.0",
    "eslint-plugin-storybook": "^0.5.12",
    "fast-glob": "^3.2.11",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.5.1",
    "jest-axe": "^6.0.0",
    "mini-css-extract-plugin": "^2.6.0",
    "postcss": "^8.4.7",
    "postcss-loader": "^6.2.1",
    "postcss-preset-env": "^7.4.2",
    "prettier": "^2.5.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "storybook-css-modules-preset": "^1.1.1",
    "style-dictionary": "^3.7.0",
    "style-loader": "^3.3.1",
    "ts-jest": "^27.1.3",
    "ts-loader": "^9.2.7",
    "ts-node": "^10.9.1",
    "typed-css-modules": "^0.7.0",
    "typescript": "^4.6.3",
    "typescript-plugin-css-modules": "^3.4.0",
    "webpack": "^5.70.0",
    "webpack-cli": "^4.9.2"
  },
  "dependencies": {
    "clsx": "^1.1.1"
  }
}
