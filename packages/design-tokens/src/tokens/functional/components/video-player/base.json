{
  "brand": {
    "VideoPlayer": {
      "closedCaption": {
        "margin": {
          "value": "0 auto 15px auto"
        },
        "text": {
          "fgColor": {
            "value": "var(--base-color-scale-white-0)"
          },
          "bgColor": {
            "value": "rgba(0, 0, 0, 0.75)"
          },
          "padding": {
            "value": "10px",
            "$type": "dimension"
          }
        }
      },
      "playButton": {
        "height": {
          "value": "var(--base-size-96)"
        },
        "width": {
          "value": "var(--base-size-96)"
        }
      },
      "range": {
        "height": {
          "value": "6px",
          "$type": "dimension"
        },
        "borderRadius": {
          "value": "3px",
          "$type": "dimension"
        },
        "minWidth": {
          "value": "75px",
          "$type": "dimension"
        }
      },
      "transition": {
        "value": "all var(--brand-animation-duration-fast) var(--brand-animation-easing-default)"
      },
      "tooltip": {
        "padding": {
          "block": {
            "value": "5px",
            "$type": "dimension"
          },
          "inline": {
            "value": "5px",
            "$type": "dimension"
          }
        }
      }
    }
  }
}
