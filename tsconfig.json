{
  "compilerOptions": {
    "declaration": true,
    "target": "esnext",
    "module": "esnext",
    "lib": ["esnext", "dom"],
    "emitDeclarationOnly": false,
    "skipLibCheck": false,
    "sourceMap": true,
    "strict": true,
    "noImplicitAny": false,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "outDir": "lib",
    "jsx": "react-jsx",
    "plugins": [{"name": "typescript-plugin-css-modules"}]
  },
  "include": ["src/**/*", "scripts/**/*"]
}
